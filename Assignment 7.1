2. Check the probability distribution of all variables in mtcars
mean(mtcars$mpg)
mtcars$mpg_cat<-ifelse(mtcars$mpg>20,yes=1,no=0)

model<-glm(mpg_cat~wt+drat+hp+cyl,data = mtcars, family = binomial)
model
fit<-lm(mpg_cat~wt+drat+hp+cyl,data = mtcars, data = mtcars)
fit
summary(fit)
predict<-predict(model,type="response")
mtcars$predict<-predict
mtcars$predictROUND<-round(predict,digits=0)
table(mtcars$mpg_cat,predict>=0.5)
table(mtcars$mpg_cat,predict>=0.6)
table(mtcars$mpg_cat,predict>=0.2)
table(mtcars$mpg_cat,predict>=0.7)

